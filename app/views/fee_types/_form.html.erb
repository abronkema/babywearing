<%= form_with(model: fee_type, local: true) do |form| %>
  <%= form_errors fee_type %>

  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :amount %>
    <%= form.text_field :amount, class: "form-control" %>
  </div>

  <%= form.submit class: "btn btn-primary" %>
<% end %>
